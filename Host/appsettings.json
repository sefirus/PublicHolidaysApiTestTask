{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ScheduleProcessingChunkSize": 50,
  "ExternalHolidayApi": {
    "BaseUrl": "https://kayaposoft.com/enrico/json/v3.0",
    "GetHolidaysForDateRangeEndpoint": "getHolidaysForDateRange",
    "GetSupportedCountriesEndpoint": "getSupportedCountries",
    "DatesForCountryNotSupportedPattern": "Dates\\s+.+?\\s+are\\s+not\\s+supported",
    "CountryNotSupportedPattern": "Country\\s+'[^']+'\\s+is\\s+not\\s+supported"
  }
}
